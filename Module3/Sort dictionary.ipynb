     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sorted dictionary is :\n",
      "[(1, 'a'), (2, 'b'), (3, 'c'), (4, 'd')]\n"
     ]
    }
   ],
   "source": [
    "my_dict = {'c': 3,'a': 1,'d': 4,'b': 2}\n",
    "\n",
    "sorted_dict = sorted([(value, key)\n",
    "\n",
    "for (key, value) in my_dict.items()])\n",
    "\n",
    "print(\"Sorted dictionary is :\")\n",
    "\n",
    "print(sorted_dict)\n"
   ]
